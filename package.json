{
  "private": false,
  "name": "vwin",
  "version": "1.0.0",
  "scripts": {
    "start": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father-build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "publish": "lerna publish from-package -m \"chore: publish\"",
    "publish:test": "lerna publish --canary"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:abelce/vwin.git",
    "branch": "main"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "antd": "^4.19.1",
    "core-decorators": "^0.20.0",
    "intersection-observer": "^0.12.0",
    "uuidv4": "^6.2.12"
  },
  "devDependencies": {
    "@types/react": "^17.0.14",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/test": "^3.0.5",
    "babel-plugin-import": "^1.13.3",
    "dumi": "^1.1.0",
    "father-build": "^1.20.4",
    "gh-pages": "^3.0.0",
    "lerna": "^4.0.0",
    "lint-staged": "^10.0.7",
    "node-sass": "^6.0.1",
    "prettier": "^1.19.1",
    "sass-loader": "^12.3.0",
    "yorkie": "^2.0.0"
  },
  "sideEffects": [
    "dist/*",
    "es/**/style/*",
    "lib/**/style/*",
    "*.scss",
    "*.css"
  ],
  "workspaces": [
    "packages/*"
  ]
}
